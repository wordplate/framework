{
    "name": "wordplate/framework",
    "description": "The WordPlate framework",
    "keywords": [
        "wordplate",
        "framework",
        "wordpress",
        "boilerplate"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Vincent Klaiber",
            "email": "hello@vinkla.com",
            "homepage": "https://vinkla.com"
        },
        {
            "name": "Chris Andersson",
            "email": "hello@puredazzle.se",
            "homepage": "https://puredazzle.se"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-mbstring": "*",
        "composer/installers": "^1.6",
        "illuminate/support": "^5.7",
        "johnpbloch/wordpress": "^5.0",
        "roots/wp-password-bcrypt": "^1.0",
        "symfony/http-foundation": "^4.1",
        "symfony/routing": "^4.2",
        "symfony/var-dumper": "^4.2",
        "vlucas/phpdotenv": "^2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "7.0-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "WordPlate\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "WordPlate\\Tests\\": "tests/"
        },
        "files": [
            "tests/helpers.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
