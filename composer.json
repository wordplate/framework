{
    "name": "wordplate/framework",
    "description": "The WordPlate framework",
    "keywords": [
        "wordplate",
        "framework",
        "wordpress",
        "boilerplate"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Vincent Klaiber",
            "email": "hello@doubledip.se",
            "homepage": "https://doubledip.se"
        },
        {
            "name": "Chris Andersson",
            "email": "hello@puredazzle.se",
            "homepage": "https://puredazzle.se"
        }
    ],
    "require": {
        "php": "^7.2",
        "composer/installers": "^1.7",
        "johnpbloch/wordpress": "^5.3",
        "roots/wp-password-bcrypt": "^1.0",
        "symfony/http-foundation": "^5.0",
        "symfony/routing": "^5.0",
        "symfony/var-dumper": "^5.0",
        "vlucas/phpdotenv": "^3.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4"
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "8.0-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "WordPlate\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "WordPlate\\Tests\\": "tests/"
        },
        "files": [
            "tests/stubs/functions.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
